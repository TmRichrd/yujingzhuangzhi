// sidebar
$menuText:#fff;
$menuActiveText:#76a1f7;
$subMenuActiveText:#76a1f7; //https://github.com/ElemeFE/element/issues/12951

$menuBg:#001640;
$menuHover:#0a3079;

$subMenuBg:#001640;
$subMenuHover:#0a3079;

$sideBarWidth: 170px;
$btnPrimary2:#177DDC;
// the :export directive is the magic sauce for webpack
// https://www.bluematador.com/blog/how-to-share-variables-between-js-and-sass
:export {
  menuText: $menuText;
  menuActiveText: $menuActiveText;
  subMenuActiveText: $subMenuActiveText;
  menuBg: $menuBg;
  menuHover: $menuHover;
  subMenuBg: $subMenuBg;
  subMenuHover: $subMenuHover;
  sideBarWidth: $sideBarWidth;
}
